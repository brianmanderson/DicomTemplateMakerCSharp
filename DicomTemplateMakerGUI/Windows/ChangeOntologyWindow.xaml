<Window x:Class="DicomTemplateMakerGUI.Windows.ChangeOntologyWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DicomTemplateMakerGUI.Windows"
        mc:Ignorable="d"
        Title="AddAirTableTemplate" Height="225" Width="500">
    <Grid>
        <Label Content="If you would like to change the default Ontology this will attempt to change the&#xD;&#xA; template ROI ontologies. It will convert from FMA to SNOMED-CT, or vice-versa" HorizontalAlignment="Center" Margin="0,10,0,0" VerticalAlignment="Top" Height="55" Width="480"/>
        <Button x:Name="ConverterButton" Content="Convert" HorizontalAlignment="Center" Height="48" Margin="0,126,0,0" VerticalAlignment="Top" Width="72" Click="ConverterButton_Click"/>
        <ComboBox Name="From_ComboBox" HorizontalAlignment="Left" Height="36" Margin="10,69,0,0" VerticalAlignment="Top" Width="177" SelectionChanged="ToFromComboBox_SelectionChanged"/>
        <ComboBox Name="To_ComboBox" HorizontalAlignment="Left" Height="36" Margin="298,69,0,0" VerticalAlignment="Top" Width="177" SelectionChanged="ToFromComboBox_SelectionChanged"/>
        <Label Content="convert to" HorizontalAlignment="Left" Margin="214,74,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
