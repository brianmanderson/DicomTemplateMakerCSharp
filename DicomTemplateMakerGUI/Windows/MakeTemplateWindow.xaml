<Window x:Class="DicomTemplateMakerGUI.Windows.MakeTemplateWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DicomTemplateMakerGUI.Windows"
        mc:Ignorable="d"
        Title="MakeTemplateWindow" Height="450" Width="800">
    <Grid>
        <Button Content="Select RT Structure" HorizontalAlignment="Left" Margin="29,40,0,0" VerticalAlignment="Top" Click="Select_File_Click" Height="37" Width="137"/>
        <Label Name="FileLocationLabel" Content="" HorizontalAlignment="Left" Height="37" Margin="165,40,0,0" VerticalAlignment="Top" Width="587"/>
        <Button x:Name="OutPath_Button" IsEnabled="False" Content="Select output path" HorizontalAlignment="Left" Margin="29,82,0,0" VerticalAlignment="Top" Click="Select_Folder_Click" Height="37" Width="137"/>
        <Label Name="FolderLocationLabel" Content="" HorizontalAlignment="Left" Height="37" Margin="171,82,0,0" VerticalAlignment="Top" Width="587"/>
        <Button x:Name="BuildButton" Content="Build!" HorizontalAlignment="Left" Height="39" Margin="29,124,0,0" VerticalAlignment="Top" Width="137" Click="Build_Button_Click" IsEnabled="False"/>
    </Grid>
</Window>
