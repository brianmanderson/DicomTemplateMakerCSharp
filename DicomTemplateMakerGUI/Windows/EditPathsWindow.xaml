<Window x:Class="DicomTemplateMakerGUI.Windows.EditPathsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DicomTemplateMakerGUI.Windows"
        mc:Ignorable="d"
        Title="EditPathsWindow" Height="250" Width="800">
    <Grid>
        <ScrollViewer VerticalScrollBarVisibility="Visible" Margin="0,10,150,0">
            <StackPanel x:Name="PathsStackPanel" Height="172"/>
        </ScrollViewer>
        <Button Content="Add Path" HorizontalAlignment="Left" Height="34" Margin="663,22,0,0" VerticalAlignment="Top" Width="101" Click="Button_Click"/>
        <Button Content="Save and Close" HorizontalAlignment="Left" Height="34" Margin="663,61,0,0" VerticalAlignment="Top" Width="101" Click="Save_Click"/>
    </Grid>
</Window>
